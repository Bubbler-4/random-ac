{
    "manifest_version": 2,
    "name": "random.ac",
    "description": "A Random Defense helper for BOJ Practice",
    "version": "0.1",
    "permissions": [
        "scripting",
        "activeTab",
        "https://solved.ac/*",
        "https://www.acmicpc.net/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.acmicpc.net/group/practice/create/*"
            ],
            "js": [
                "scripts/content.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "background": {
        "scripts": ["scripts/service-worker.js"]
    }
}